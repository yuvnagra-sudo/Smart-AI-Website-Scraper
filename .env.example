# ============================================================
# VC Enrichment Web - Environment Variables
# Copy this file to .env and fill in your values.
# ============================================================

# --- Database ---
# MySQL connection string (required for all saves to work)
DATABASE_URL=mysql://user:password@host:3306/dbname

# --- OpenAI ---
# Required for AI-powered data extraction (investment niches, team members, etc.)
OPENAI_API_KEY=sk-...
# Optional: override the model used for all LLM calls (default: gpt-4o-mini)
# OPENAI_MODEL=gpt-4o-mini

# --- Jina AI (website fetching) ---
# Optional but strongly recommended â€” Jina API key for fast website content extraction
# Without this, falls back to Puppeteer (slower, resource-heavy)
JINA_API_KEY=jina_...

# --- File Storage (Manus Forge API) ---
# Required for uploading input Excel files and storing results
BUILT_IN_FORGE_API_URL=https://...
BUILT_IN_FORGE_API_KEY=...

# --- OAuth (Manus platform login) ---
# Required for production login. Not needed when DEV_BYPASS_AUTH=true.
OAUTH_SERVER_URL=https://...
OWNER_OPEN_ID=...
VITE_APP_ID=...
JWT_SECRET=some-long-random-secret-string

# --- Vayne API (optional enrichment provider) ---
VAYNE_API_KEY=...

# --- Development ---
# Set to "true" to bypass Manus OAuth and use a hardcoded dev user.
# NEVER set this in production.
# DEV_BYPASS_AUTH=true
NODE_ENV=development
